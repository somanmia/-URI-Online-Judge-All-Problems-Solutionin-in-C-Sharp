using System;
namespace array
{
    class URI
    {
        static void Main(string[] args)
        {
            int i;
            int[] arr = new int[20];
            for (i = 19; i >= 0; i--) {
                arr[i] = Convert.ToInt32(Console.ReadLine());
            }
            for (i = 0; i < arr.Length; i++) {
                Console.WriteLine("N[{0}] = {1}", i, arr[i]);
            }
            Console.ReadKey();
           
        }
    }
}
